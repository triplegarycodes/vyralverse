{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "react",
    "lib": [
      "DOM",
      "ES2019"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strict": true,
    "types": [
      "react",
      "react-native",
      "jest"
    ],
    "baseUrl": ".",
    "paths": {
      "@core/*": [
        "src/core/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
